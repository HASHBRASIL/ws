<h2> {{grupo.nome}} {{msg}} </h2>
<input type="hidden" id="iditem" value="{{id}}" />
<div class='grupoDivPropriedades'>
    <table class="table table-responsive">
        <tr>
            <th>Propriedade</th>
            <th>valor</th>
        </tr>
        {% for prop, valor in itensReadonly %}
            <tr>
                <td>{{prop}}</td>
                <td>
                    {{valor}}
                </td>
            </tr>
        {% endfor %}
    </table>
    
    <table class="table table-responsive tabela-filtros">
        <tr>
            <th>Propriedade</th>
            <th>valor</th>
        </tr>
        {% for prop, valor in itens %}
            <tr>
                <td>{{prop}}</td>
                <td>
                    <input coluna='{{prop}}' value='{{valor}}' type="text" />
                </td>
            </tr>
        {% endfor %}
    </table>
</div>
{% include 'prop-filtro.html.twig' %}
{% include 'prop-metadata.html.twig' %}